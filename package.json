{
  "name": "iterra-player",
  "version": "1.0.7",
  "main": "electron-main.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:electron": "ng build --base-href ./ && electron .",
    "build": "ng build",
    "build:electron": "ng build --base-href ./ && electron-builder build --win --publish never",
    "deploy:electron": "electron-builder build --win --publish always",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "nsis": {
    "deleteAppDataOnUninstall": true
  },
  "build": {
    "appId": "iterrait/player-app",
    "linux": {
      "target": "deb",
      "icon": "build/32x32.png"
    },
    "deb": {},
    "asar": false,
    "publish": [
      {
        "provider": "github",
        "owner": "iterrait",
        "repo": "player-app"
      }
    ]
  },
  "dependencies": {
    "@types/electron": "^1.6.10",
    "electron-updater": "^5.3.0",
    "node-schedule": "^2.1.1"
  },
  "devDependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "^16.1.2",
    "@angular/cli": "~16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/compiler-cli": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular-devkit/build-angular": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@types/jasmine": "~4.3.0",
    "@angular/material": "^16.1.2",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@types/uuid": "^9.0.2",
    "angular-imask": "^7.1.0-alpha.0",
    "bootstrap": "^5.3.0",
    "electron": "^25.1.1",
    "electron-builder": "^24.6.3",
    "ngx-mat-timepicker": "^16.0.2",
    "ngx-material-timepicker": "^13.1.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typescript": "~5.1.3",
    "uuid": "^9.0.0",
    "zone.js": "~0.13.0"
  }
}
